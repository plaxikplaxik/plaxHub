local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Jxereas/UI-Libraries/main/cerberus.lua"))()
local Humanoid = game.Players.LocalPlayer.Character.Humanoid
local window = Library.new("plax Hub")
window:LockScreenBoundaries(true)
-- Tabs
local tab_main = window:Tab("Main")

-- Player Section
local section_player = tab_main:Section("Player")

local WalkSpeedEnabled
local WalkSpeedval = 16
section_player:Slider("WalkSpeed",function(val) if WalkSpeedEnabled == true then Humanoid.WalkSpeed = val WalkSpeedval = val end end,150,16) -- Walk Speed
local toggle_WalkSpeed = section_player:Toggle("Toggle WalkSpeed", function(bool)
    WalkSpeedEnabled = bool
    if bool == false then Humanoid.WalkSpeed = 16 else Humanoid.WalkSpeed = WalkSpeedval end
end)
toggle_WalkSpeed:Set(false)
